# Integration Test Environment Configuration
# Copy to .env.test and update with your test environment values

# PostgreSQL Test Configuration
POSTGRES_HOST=192.168.1.104
POSTGRES_PORT=5432
POSTGRES_DB=codex_db
POSTGRES_USER=codex_user
POSTGRES_PASSWORD=your_postgres_password_here

# Test Database Configuration (for isolated testing)
TEST_DB_NAME=codex_test_db
TEST_DB_USER=codex_test_user
TEST_DB_PASS=your_test_password_here

# Ollama Configuration  
OLLAMA_URL=http://192.168.1.110:11434
OLLAMA_MODEL=gpt-oss:20b
EMBEDDING_MODEL=nomic-embed-text
OLLAMA_GENERATION_TIMEOUT_SECONDS=300
EMBEDDING_TIMEOUT_SECONDS=300

# DuckDB Configuration
DUCKDB_PATH=./test_memory.duckdb
LLM_CACHE_PATH=./test_llm_cache.duckdb

# Performance Testing Thresholds
MAX_QUERY_TIME_MS=50
MAX_EMBEDDING_TIME_MS=5000
MAX_LLM_GENERATION_TIME_MS=30000

# Test Configuration
TEST_TIMEOUT_SECONDS=300
TEST_BATCH_SIZE=10
TEST_MAX_RETRIES=3